{
  "name": "youtube-timestamp-bookmarker",
  "version": "1.0.0",
  "description": "A Chrome extension to bookmark YouTube timestamps",
  "packageManager": "bun@1.2.21",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "check:staged": "biome check --changed",
    "check:staged:fix": "biome check --changed --write",
    "ci": "biome check . && vitest run",
    "perf:gen": "bun scripts/generate-sample-bookmarks.mjs > sample-bookmarks.json"
  },
  "bin": {},
  "keywords": [
    "chrome-extension",
    "youtube",
    "timestamp",
    "bookmark"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^27.0.0",
    "vitest": "^3.2.4"
  }
}
